{
  "name": "@peterbd/vision-camera-face-detector",
  "version": "0.1.15",
  "description": "VisionCamera Frame Processor Plugin to detect faces using MLKit Vision Face Detector",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "vision-camera-face-detector.podspec"
  ],
  "scripts": {
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build",
    "prepack": "bob build",
    "release": "release-it",
    "example": "yarn --cwd example",
    "format": "prettier --write \"src/**/*.ts\"",
    "bootstrap": "yarn example && yarn install && yarn example pods",
    "clean": "del-cli android/build example/android/build example/android/app/build example/ios/build"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "vision-camera",
    "face-detector",
    "mlkit"
  ],
  "repository": "https://github.com/peterbd/vision-camera-face-detector.git",
  "author": "Matthew Terry <matthewterry68.mt@gmail.com> (https://github.com/mat2718)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/peterbd/vision-camera-face-detector/issues"
  },
  "homepage": "https://github.com/peterbd/vision-camera-face-detector#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^11.0.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@release-it/conventional-changelog": "^2.0.0",
    "@types/jest": "^26.0.0",
    "@types/react": "^18.0.26",
    "@types/react-native": "^0.70.8",
    "commitlint": "^11.0.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "pod-install": "^0.1.0",
    "prettier": "^2.0.5",
    "react": "17.0.1",
    "react-native": "0.72.5",
    "react-native-builder-bob": "^0.18.1",
    "react-native-reanimated": "2.2.0",
    "react-native-vision-camera": "3.6.4",
    "release-it": "^14.2.2",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-vision-camera": ">= 2"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/betaExample/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "dependencies": {
    "react-native-worklets-core": "^0.3.0"
  }
}
